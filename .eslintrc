{
  "extends": [
    "next",
    "eslint:recommended"
  ],
  "env": {
    "es6": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "no-unused-vars": "warn"
    // "jsx-a11y/alt-text": "off",
    // "react/jsx-no-target-blank": "off",
    // "import/no-anonymous-default-export": "off"
  },
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {
        "no-undef": "off"
      }
    }
  ]
}